{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}",
      "timeout": 15000,
      "restart": true,
      "showAsyncStacks": true,
      "smartStep": true
    },
    {
      "type": "vscode-edge-devtools.debug",
      "request": "attach",
      "name": "Attach to Microsoft Edge and open the Edge DevTools",
      "url": "http://localhost:9223",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Attach to Edge",
      "port": 9223,
      "request": "attach",
      "type": "msedge",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Main",
      "port": 5858,
      "request": "attach",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "type": "node",
      "timeout": 15000,
      "restart": {
        "delay": 1000,
        "maxAttempts": 10
      },
      "showAsyncStacks": true,
      "outFiles": ["${workspaceFolder}/**/*.js", "!**/node_modules/**"]
    },
    {
      "name": "Launch Program",
      "program": "C:\\Users\\Sandakan Nipunajith\\OneDrive\\Desktop\\Untitled-1.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ],
  "compounds": [
    {
      "name": "Main & Renderer",
      "configurations": ["Main", "Renderer"]
    },
    {
      "name": "Launch Edge Headless and attach DevTools",
      "configurations": [
        "Launch Microsoft Edge in headless mode",
        "Open Edge DevTools"
      ]
    },
    {
      "name": "Launch Edge and attach DevTools",
      "configurations": ["Launch Microsoft Edge", "Open Edge DevTools"]
    }
  ]
}
