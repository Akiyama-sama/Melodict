{
  "name": "nora",
  "description": "An elegant music player built using Electron and React. Inspired by Oto Music for Android by Piyush Mamidwar.",
  "version": "1.0.0-alpha.20230207",
  "license": "MIT",
  "appPreferences": {
    "removeReactStrictMode": false,
    "supportedMusicExtensions": [
      "mp3",
      "wav",
      "ogg"
    ]
  },
  "releaseNotes": {
    "md": "https://raw.githubusercontent.com/Sandakan/Nora/master/changelog.md",
    "json": "https://raw.githubusercontent.com/Sandakan/Nora/master/release-notes.json"
  },
  "urls": {
    "raw_repository_url": "https://raw.githubusercontent.com/Sandakan/Nora/"
  },
  "homepage": "https://github.com/Sandakan/Nora",
  "bugs": {
    "url": "https://github.com/Sandakan/Nora/issues",
    "email": "sandakannipunajith@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sandakan/Nora"
  },
  "author": {
    "name": "Sandakan Nipunajith",
    "email": "adsandakannipunajith2003@outlook.com",
    "url": "https://github.com/Sandakan"
  },
  "keywords": [
    "music",
    "electron",
    "react",
    "typescript",
    "music-player",
    "lyrics",
    "windows",
    "desktop"
  ],
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "prepare": "husky install",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon --inspect=5858 --remote-debugging-port=9223 --trace-warnings -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "didyoumean2": "^5.0.0",
    "electron-store": "^8.1.0",
    "music-metadata": "^7.13.3",
    "node-fetch": "^2.6.8",
    "node-id3": "^0.2.5",
    "node-vibrant": "^3.2.1-alpha.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.8",
    "songlyrics": "^2.4.4"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/auto-launch": "^5.0.2",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/node-fetch": "2.6.2",
    "@types/react": "^18.0.27",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "^18.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@types/sharp": "^0.31.1",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^7.6.0",
    "core-js": "^3.27.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "detect-port": "^1.5.1",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "electron": "^20.3.11",
    "electron-builder": "^23.6.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.2.2",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "file-type": "^18.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "terser": "^5.16.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "overrides": {
    "node-vibrant": {
      "@vibrant/image-node": {
        "@jimp/types": {
          "@jimp/jpeg": {
            "jpeg-js": "^0.4.4"
          }
        }
      }
    },
    "electron": {
      "@electron/get": "^2.0.0"
    },
    "minimatch": "^3.0.5",
    "loader-utils": "^1.4.2",
    "json5": "^2.2.2"
  },
  "build": {
    "productName": "Nora",
    "copyright": "Copyright Â© 2022 Sandakan Nipunajith",
    "appId": "com.SandakanNipunajith.Nora",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "fileAssociations": [
      {
        "ext": [
          "mp3",
          "wav",
          "ogg"
        ],
        "role": "Editor",
        "icon": "./assets/images/nora-associated-file-icon.ico"
      }
    ],
    "protocols": {
      "name": "nora-protocol",
      "schemes": [
        "nora"
      ],
      "role": "Viewer"
    },
    "nsis": {
      "perMachine": false,
      "oneClick": false,
      "allowElevation": false,
      "createDesktopShortcut": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./assets/icon.ico",
      "uninstallerIcon": "./assets/icon.ico",
      "installerSidebar": "./assets/installer_assets/sidebar.bmp",
      "license": "./LICENSE.txt"
    },
    "icon": "./assets/icon.ico",
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**",
      "!**/assets/images/other/*",
      "!**/assets/installer_assets/*",
      "!**/assets/other/*",
      "!**/assets/*.ts"
    ],
    "publish": {
      "provider": "github",
      "owner": "Sandakan Nipunajith"
    }
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ]
  }
}
